[
    // Passthrough keybindings
    { "key": "alt+j", "command": "vscode-neovim.send", "when": "neovim.init && editorTextFocus", "args": "<A-j>", },
    { "key": "alt+k", "command": "vscode-neovim.send", "when": "neovim.init && editorTextFocus", "args": "<A-k>", },
    { "key": "ctrl+h", "command": "vscode-neovim.send", "when": "neovim.init && neovim.mode == 'normal' && !editorTextFocus", "args": "<C-h>", },
    { "key": "ctrl+j", "command": "vscode-neovim.send", "when": "neovim.init && neovim.mode == 'normal' && !editorTextFocus", "args": "<C-j>", },
    { "key": "ctrl+k", "command": "vscode-neovim.send", "when": "neovim.init && neovim.mode == 'normal' && !editorTextFocus", "args": "<C-k>", },
    { "key": "ctrl+l", "command": "vscode-neovim.send", "when": "neovim.init && neovim.mode == 'normal' && !editorTextFocus", "args": "<C-l>", },
    { "key": "space b", "command": "vscode-neovim.send", "when": "neovim.init && neovim.mode == 'normal' && !editorFocus && !inputFocus", "args": "<Space>b", },
    { "key": "space shift+e", "command": "vscode-neovim.send", "when": "neovim.init && neovim.mode == 'normal' && !editorFocus && !inputFocus", "args": "<Space>E", },
    { "key": "space shift+f", "command": "vscode-neovim.send", "when": "neovim.init && neovim.mode == 'normal' && !editorFocus && !inputFocus", "args": "<Space>F", },
    { "key": "space g", "command": "vscode-neovim.send", "when": "neovim.init && neovim.mode == 'normal' && !editorFocus && !inputFocus", "args": "<Space>g", },
    { "key": "space f f", "command": "vscode-neovim.send", "when": "neovim.init && neovim.mode == 'normal' && !editorFocus && !inputFocus", "args": "<Space>ff", },
    { "key": "space p", "command": "vscode-neovim.send", "when": "neovim.init && neovim.mode == 'normal' && !editorFocus && !inputFocus", "args": "<Space>p", },
    { "key": "space j", "command": "vscode-neovim.send", "when": "neovim.init && neovim.mode == 'normal' && !editorFocus && !inputFocus", "args": "<Space>j", },
    { "key": "space k s", "command": "vscode-neovim.send", "when": "neovim.init && neovim.mode == 'normal' && !inKeyBindings && !editorFocus && !inputFocus", "args": "<Space>ks" },
    { "key": "space e n", "command": "vscode-neovim.send", "when": "neovim.init && neovim.mode == 'normal' && !editorFocus && !inputFocus", "args": "<Space>en", },
    { "key": "space e d", "command": "vscode-neovim.send", "when": "neovim.init && neovim.mode == 'normal' && !editorFocus && !inputFocus", "args": "<Space>ed", },

    // Keybindings editor navigation
    { "key": "escape", "command": "keybindings.editor.focusKeybindings", "when": "neovim.init && inKeybindings && inKeybindingsSearch", },
    { "key": "i", "command": "keybindings.editor.searchKeybindings", "when": "neovim.init && inKeybindings && !inputFocus", },
    {
        "key": "escape",
        "command": "workbench.action.closeActiveEditor",
        "when": "neovim.init && inKeybindings && !inputFocus",
    },

    // Panel navigation
    {
        "key": "tab",
        "command": "workbench.action.nextPanelView",
        "when": "panelFocus",
    },
    {
        "key": "shift+tab",
        "command": "workbench.action.previousPanelView",
        "when": "panelFocus",
    },
]
